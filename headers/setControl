#define SETCALC_HEADERS_SETCONTROL_

#include <algorithm>
#include <format>
#include <iostream>
#include <vector>

void size(std::pmr::vector<long double> &vec);
void showset(std::pmr::vector<long double> &vec);
void reset(std::pmr::vector<long double> &vec);

#endif
